// CSP 2025入门组第二轮 第1题

import "os"

func main {
	solve(os.StdinData())
}

func solve(s: []byte) {
	digits := make([]int, 10)

	// 统计数字的个数
	for _, c := range s {
		if c >= '0' && c <= '9' {
			digits[c-'0']++
		}
	}

	// 倒序输出
	for i := 9; i >= 0; i-- {
		for k := 0; k < digits[i]; k++ {
			print(string('0' + i))
		}
	}
}
